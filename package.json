		"@storybook/addon-storysource": "5.3.17",
		"@storybook/addons": "5.3.17",
		"@storybook/angular": "5.3.17",
		"@types/jasmine": "2.8.2",
		"@types/node": "13.9.1",
		"autoprefixer": "8.1.0",
		"babel-loader": "8.0.6",
		"carbon-components": "10.10.1",
		"carbon-components-angular": "3.46.2",
		"codelyzer": "4.5.0",
		"core-js": "2.5.4",
		"husky": "4.2.3",
		"jasmine-core": "2.99.1",
		"jasmine-spec-reporter": "4.2.1",
		"karma": "4.4.1",
